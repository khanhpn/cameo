<div class="row statistic-bottom">
  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 text-center">
    <%= form_with url: statistic_path, method: :get, local: true do |form| %>
      <div class="form-group">
        <label><h4>Choose datetime range</h4></label>
        <%= form.text_field :date_range, id: "id_date_range", "data-datetime": datetime_range(@start_time, @end_time), class: "form-control text-center", placeholder: "Choose datetime range" %>
      </div>
      <%= form.submit "Search", class: "btn btn-primary", data: { disable_with: false } %>
    <% end %>
  </div>
</div>
<div id="id_statistic_stars" class="row">
  <%= render partial: "statistic_star/data", locals: {pagy: @pagy, categories: @categories, start_time: @start_time, end_time: @end_time} %>
</div>
