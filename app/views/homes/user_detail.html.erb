<div class="row">
  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
    <table class="table table-bordered">
      <tbody>
        <tr>
          <th>Id</th>
          <td scope="row"><%= @user.id %></td>
        </tr>
        <tr>
          <th>_id</th>
          <td><%= @user._id %></td>
        </tr>
        <tr>
          <th>name</th>
          <td><%= @user.name %></td>
        </tr>
        <tr>
          <th>username</th>
          <td><%= @user.username %></td>
        </tr>
        <tr>
          <th>engagement_speed</th>
          <td><%= @user.engagement_speed %></td>
        </tr>
        <tr>
          <th>engagement_volume</th>
          <td><%= @user.engagement_volume %></td>
        </tr>
        <tr>
          <th>engagement_score</th>
          <td><%= @user.engagement_score %></td>
        </tr>
        <tr>
          <th>firstOrderCompletedAt</th>
          <td><%= @user.firstOrderCompletedAt %></td>
        </tr>
        <tr>
          <th>averageMillisecondsToComplete</th>
          <td><%= @user.averageMillisecondsToComplete %></td>
        </tr>
        <tr>
          <th>Image</th>
          <td><%= image_tag @user.imageUrl, class: "rounded-circle image" %></td>
        </tr>
        <tr>
          <th>imageUrlKey</th>
          <td><%= @user.imageUrlKey %></td>
        </tr>
        <tr>
          <th>numOfRatings</th>
          <td><%= @user.numOfRatings %></td>
        </tr>
        <tr>
          <th>averageRating</th>
          <td><%= @user.averageRating %></td>
        </tr>
        <tr>
          <th>price</th>
          <td><%= @user.price %></td>
        </tr>
        <tr>
          <th>profession</th>
          <td><%= @user.profession %></td>
        </tr>
        <tr>
          <th>bio</th>
          <td><%= @user.bio %></td>
        </tr>
        <tr>
          <th>Category</th>
          <td><%= show_category(@user.categories) %></td>
        </tr>
        <tr>
          <th>Created at</th>
          <td><%= @user.created_at %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
